(defun pair-dist (a b)
  (+ (abs (- (car a) (car b)))
     (abs (- (cadr a) (cadr b)))))

(defun all-dist (a)
  (when a
    (append
     (mapcar (lambda (b)
	       (pair-dist (car a) b))
	     (cdr a))
     (all-dist (cdr a)))))

